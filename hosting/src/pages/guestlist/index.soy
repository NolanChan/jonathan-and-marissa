---
description: "RSVP Guestlist"
title: "Guestlist"
hidden: true
---

{namespace guestlist}

/**
 * 
 */
{template .render}
	{call main.render data="all"}
		{param content kind="html"}
			{call .guestlist /}
		{/param}
	{/call}
{/template}

/**
 * 
 */
{template .guestlist}
	<div class="guestlist">
		<div class="container container-fluid">
			<form id="section-container">
				<h2>GUEST LIST</h2>
				<table class=" table">
					<thead>
						<th class="first_name">First</th>
						<th class="last_name">Last</th>
						<th class="attending">Attending</th>
						<th class="plus_one">Plus One</th>
						<th class="gluten">Gluten-Free</th>
						<th class="dairy">Dairy-Free</th>
					</thead>
					<tbody class="rsvpList"></tbody>
				</table>
			</form>
		</div>
	</div>
	<footer></footer>
	<script src="/js/guestlist.js"></script>
{/template}






